{
    "name": "Dark",
    "colors": {
        "activityBar.background": "#2b2d30",
        "activityBar.border": "#1e1f22",
        "activityBar.activeBackground": "#3574f0",
        "activityBar.activeBorder": "#00000000",
        "activityBar.activeForeground": "#3574f0",
        "editor.background": "#1e1f22",
        "editor.foreground": "#bcbec4",
        "editor.lineHighlightBackground": "#26282E",
        "editorCursor.foreground": "#CED0D6",
        "editorLineNumber.activeForeground": "#A1A3AB",
        "editorLineNumber.foreground": "#4B5059",
        "list.activeSelectionBackground": "#2e436e",
        "list.activeSelectionForeground": "#FFFFFF",
        "list.inactiveSelectionBackground": "#264F7850",
        "list.focusOutline": "#ffffff00",
        "list.hoverBackground": "#2A2D2E",
        "sideBar.background": "#2b2d30",
        "statusBar.background": "#2b2d30",
        "statusBar.border": "#1e1f22",
        "statusBarItem.hoverBackground": "#37373D",
        "statusBarItem.remoteBackground": "#2b2d30",
        "titleBar.background": "#3574f0",
        "titleBar.border": "#1e1f22",
        "tree.indentGuidesStrokeWidth": "10px",
        "list.activeSelectionOutline": "transparent"
    },
    "tokenColors": [
        {
            "name": "Default Text",
            "scope": [
                "source",
                "text"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#6272A4",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Keyword & Storage",
            "scope": [
                "keyword",
                "storage",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "Annotation",
            "scope": [
                "meta.annotation",
                "meta.annotation.identifier",
                "punctuation.definition.annotation",
                "storage.type.annotation"
            ],
            "settings": {
                "foreground": "#FFB86C"
            }
        },
        {
            "name": "Class & Type",
            "scope": [
                "entity.name.type.class",
                "entity.name.type.interface",
                "entity.name.type.enum",
                "entity.name.type.annotation",
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "name": "Function & Method Declaration",
            "scope": [
                "entity.name.function",
                "entity.name.function.java",
                "meta.method.java",
                "meta.definition.method.java"
            ],
            "settings": {
                "foreground": "#50FA7B"
            }
        },
        {
            "name": "Function & Method Calls",
            "scope": [
                "meta.method-call.java",
                "meta.function-call.java"
            ],
            "settings": {
                "foreground": "#50FA7B"
            }
        },
        {
            "name": "Fields, Variables, Parameters",
            "scope": [
                "variable",
                "variable.parameter",
                "variable.other.local",
                "variable.other.field",
                "variable.other.object.property",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.quoted",
                "string.quoted.double.java"
            ],
            "settings": {
                "foreground": "#F1FA8C"
            }
        },
        {
            "name": "Constant & Number",
            "scope": [
                "constant",
                "constant.numeric",
                "constant.language",
                "constant.character"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Boolean & Enum Constant",
            "scope": [
                "constant.language.boolean",
                "variable.language",
                "support.constant"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "Generic Type Parameters",
            "scope": [
                "meta.generic.java",
                "meta.type.parameters"
            ],
            "settings": {
                "foreground": "#8BE9FD"
            }
        },
        {
            "name": "Import & Package",
            "scope": [
                "meta.import",
                "meta.package",
                "storage.modifier.import",
                "storage.modifier.package"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Punctuation",
            "scope": [
                "punctuation",
                "punctuation.definition",
                "punctuation.separator",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "Operators",
            "scope": [
                "keyword.operator",
                "keyword.operator.assignment",
                "keyword.operator.arithmetic",
                "keyword.operator.logical"
            ],
            "settings": {
                "foreground": "#FF79C6"
            }
        },
        {
            "name": "This / Super",
            "scope": [
                "variable.language.this",
                "variable.language.super"
            ],
            "settings": {
                "foreground": "#BD93F9"
            }
        },
        {
            "name": "tag xml",
            "scope": [
                "punctuation.definition.tag.xml"
            ],
            "settings": {
                "foreground": "#F8F8F2"
            }
        },
        {
            "name": "String (merged)",
            "scope": [
                "string"
            ],
            "settings": {
                "foreground": "#6aab73"
            }
        },
        {
            "name": "Keyword (merged)",
            "scope": [
                "keyword"
            ],
            "settings": {
                "foreground": "#cf8e6d"
            }
        },
        {
            "name": "Number (merged)",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#2aacb8"
            }
        },
        {
            "name": "Function (merged)",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#56a8f5"
            }
        },
        {
            "name": "Class (merged)",
            "scope": [
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "foreground": "#bcbec4"
            }
        }
    ]
}